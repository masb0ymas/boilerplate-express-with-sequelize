{
  "name": "boilerplate-express-with-sequelize",
  "version": "0.2.0",
  "description": "Just Boilerplate",
  "private": true,
  "scripts": {
    "start": "nodemon --exec babel-node ./src/bin/server.js",
    "lint": "npx eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "refresh:env-jwt": "node ./src/scripts/generateEnvJWT.js",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && babel src --out-dir dist",
    "serve:staging": "NODE_ENV=staging node ./dist/bin/server.js",
    "serve:production": "NODE_ENV=production node ./dist/bin/server.js",
    "db:reset": "npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "masb0ymas",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "handlebars": "^4.5.3",
    "helmet": "^3.21.3",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mariadb": "^2.2.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.0.2",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.4",
    "sequelice-query": "^1.0.2",
    "sequelize": "^6.3.1",
    "sequelize-cli": "^5.5.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "uno-api": "^1.2.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "2.0.5"
  }
}
